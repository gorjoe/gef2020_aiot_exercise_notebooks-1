    "def quadratic(a,b,c)\n",
    "    delta = b**2 - 4*a*c\n",
    "    \n",
    "    if delta<0\n",
    "        return None\n",
    "    else:\n",
    "        x1 = (-b + (delta) ** (1/2)) / (2*a)\n",
    "        x2 = (-b - (delta) ** (1/2)) / (2*a)\n",
    "        return x1,x2"
   ]
  }
